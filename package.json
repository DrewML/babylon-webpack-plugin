{
  "name": "babylon-webpack-plugin",
  "version": "0.1.0",
  "description": "Use Babylon as webpack's parser, and avoid a double parse when using babel-loader",
  "main": "BabylonWebpackPlugin.js",
  "scripts": {
    "example": "rm -rf example-project/dist && webpack --config example-project/webpack.config.js"
  },
  "keywords": [
    "webpack",
    "babel",
    "ast",
    "parser"
  ],
  "author": "Andrew Levine",
  "license": "MIT",
  "dependencies": {
    "babylon": "^7.0.0-beta.36",
    "lazy-babylon-to-estree": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.36",
    "@babel/plugin-transform-classes": "^7.0.0-beta.36",
    "babel-loader": "^8.0.0-beta.0",
    "webpack": "^4.0.0-alpha.2",
    "webpack-cli": "^2.0.1"
  }
}
